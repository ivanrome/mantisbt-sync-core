<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:batch="http://www.springframework.org/schema/batch"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
	http://www.springframework.org/schema/batch http://www.springframework.org/schema/batch/spring-batch.xsd
	http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

	<context:annotation-config />

	<batch:job id="syncEnumsJob">
		<batch:step id="customFieldTypesStep" next="etasStep">
			<batch:tasklet>
				<batch:chunk reader="customFieldTypesReader" writer="customFieldTypesWriter"
					commit-interval="10" />
			</batch:tasklet>
		</batch:step>

		<batch:step id="etasStep" next="prioritiesStep">
			<batch:tasklet>
				<batch:chunk reader="etasReader" writer="etasWriter"
					commit-interval="10" />
			</batch:tasklet>
		</batch:step>

		<batch:step id="prioritiesStep" next="projectionsStep">
			<batch:tasklet>
				<batch:chunk reader="prioritiesReader" writer="prioritiesWriter"
					commit-interval="10" />
			</batch:tasklet>
		</batch:step>

		<batch:step id="projectionsStep" next="projectViewStatesStep">
			<batch:tasklet>
				<batch:chunk reader="projectStatusReader" writer="projectStatusWriter"
					commit-interval="10" />
			</batch:tasklet>
		</batch:step>

		<batch:step id="projectViewStatesStep" next="reproducibilitiesStep">
			<batch:tasklet>
				<batch:chunk reader="projectViewStatesReader" writer="projectViewStatesWriter"
					commit-interval="10" />
			</batch:tasklet>
		</batch:step>

		<batch:step id="reproducibilitiesStep" next="resolutionsStep">
			<batch:tasklet>
				<batch:chunk reader="reproducibilitiesReader" writer="reproducibilitiesWriter"
					commit-interval="10" />
			</batch:tasklet>
		</batch:step>

		<batch:step id="resolutionsStep" next="severitiesStep">
			<batch:tasklet>
				<batch:chunk reader="resolutionsReader" writer="resolutionsWriter"
					commit-interval="10" />
			</batch:tasklet>
		</batch:step>

		<batch:step id="severitiesStep">
			<batch:tasklet>
				<batch:chunk reader="severitiesReader" writer="severitiesWriter"
					commit-interval="10" />
			</batch:tasklet>
		</batch:step>
	</batch:job>

</beans>
